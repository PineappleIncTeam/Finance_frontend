{
	"name": "freenance-app",
	"version": "2.29.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build && npm run build-sb && npm run buildEcosystem",
		"start": "next start",
		"preCommitLint": "eslint --quiet --ext .js,.jsx,.ts,.tsx,.json ./src ./scripts",
		"prePushBuild": "next build",
		"sb:dev": "storybook dev -p 6006",
		"sb:prod": "npx http-server ./storybook-static -p 8080",
		"typedoc:dev": "start ./docs/index.html",
		"typedoc:prod": "npx http-server ./docs -p 8081",
		"buildEcosystem": "tsc ecosystem.config.ts --skipLibCheck && node scripts/postBuildEcosystem.js",
		"build-sb": "storybook build",
		"build-typedoc": "npx typedoc --plugin typedoc-github-theme",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./scripts",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc",
		"index": "git add ."
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.11.2",
		"@sentry/nextjs": "^10.30.0",
		"@vkid/sdk": "^2.6.2",
		"air-datepicker": "^3.6.0",
		"axios": "^1.13.2",
		"chart.js": "^4.5.1",
		"check-password-strength": "^3.0.0",
		"classnames": "^2.5.1",
		"date-fns": "^4.1.0",
		"moment": "^2.30.1",
		"next": "16.0.10",
		"next-runtime-env": "^3.3.0",
		"pkce-challenge": "^5.0.1",
		"pm2": "^6.0.14",
		"react": "^19.2.3",
		"react-chartjs-2": "^5.3.1",
		"react-datepicker": "^7.6.0",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.68.0",
		"react-redux": "^9.2.0",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.4.2",
		"sass": "^1.96.0",
		"sharp": "^0.34.5",
		"typedoc": "^0.28.15",
		"typedoc-github-theme": "^0.3.1"
	},
	"overrides": {
		"require-in-the-middle": "8.0.1"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/eslintrc": "^3.3.3",
		"@eslint/js": "^9.39.2",
		"@storybook/addon-docs": "^10.1.9",
		"@storybook/addon-onboarding": "^10.1.9",
		"@storybook/nextjs": "^10.1.9",
		"@types/node": "^20.19.27",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.46.1",
		"@typescript-eslint/parser": "^8.46.1",
		"babel-plugin-react-compiler": "^1.0.0",
		"baseline-browser-mapping": "^2.9.7",
		"eslint": "^9.39.2",
		"eslint-config-next": "^16.0.10",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-hooks": "^0.4.3",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-redux": "^4.2.2",
		"eslint-plugin-solid": "^0.14.5",
		"eslint-plugin-sonarjs": "^3.0.5",
		"eslint-plugin-storybook": "^10.1.9",
		"http-server": "^14.1.1",
		"pre-commit": "^1.2.2",
		"pre-push": "^0.1.4",
		"prettier": "^3.7.4",
		"storybook": "^10.1.9",
		"typescript": "^5.9.3"
	},
	"license": "ISC",
